{
  "expo": {
    "name": "react-native-monster",
    "slug": "react-native-monster",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "reactnativemonster",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen"
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
